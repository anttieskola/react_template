{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "globals": {
        "expect": true,
        "LOG": true
    },
    "rules": {
        "indent": ["error", 2],
        "import/no-named-as-default": 0,
        "react/prefer-stateless-function": 0
    }
}
